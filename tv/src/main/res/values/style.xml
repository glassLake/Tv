<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.BottomSheetDialog.Round.Top" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/Style.BottomSheetDialog.Round.Top</item>
    </style>

    <style name="Style.BottomSheetDialog.Round.Top" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="Toolbar.Rtmp" parent="Widget.MaterialComponents.Toolbar.Primary">
        <item name="android:elevation">0dp</item>
    </style>

    <style name="Widget.Rtmp.Switch" parent="Widget.MaterialComponents.CompoundButton.Switch">
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:padding">16dp</item>
        <item name="android:minHeight">?android:attr/listPreferredItemHeightSmall</item>
    </style>

    <style name="Widget.TextView.Primary" parent="">
        <item name="android:padding">16dp</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="TextAppearance.Rtmp.MaterialComponents.Subtitle1" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="android:textStyle">normal</item>
    </style>

    <style name="NavigationView.Rtmp" parent="Widget.Design.NavigationView">
        <item name="itemIconTint" >@color/white</item>
        <item name="itemTextColor" >@color/white</item>
        <item name="android:background" >@android:color/transparent</item>
        <item name="insetForeground" >@android:color/transparent</item>
    </style>

</resources>